  grouping media-stream-band-info {
    description
      "st_mediaStream_bandInfo";
    leaf multicast-direct {
      type boolean;
      default "false";
      description
        "This object specifies the decision flag to allow multicast direct stream on radio.";
    }
    leaf multicast-direct-adm-best-effort {
      type boolean;
      default "false";
      description
        "This object specifies whether to admit a media stream in best effort queue.";
    }
    leaf video-redirect {
      type boolean;
      default "false";
      description
        "This object specifies whether a unicast or multicast only video stream can be redirected to best effort.";
    }
    leaf cac-mmedia-max-bandwidth {
      type uint32 {
        range "5 .. 85";
      }
      default "85";
      description
        "This object specifies the maximum allowed bandwidth for media traffic.";
    }
    leaf cac-mstream-mdirect-min-client-rate {
      type wireless-enum-types:dot11-min-phy-rate;
      default "six";
      description
        "cacMStreamMDirectMinClientRate";
    }
    leaf cac-mstream-mdirect-max-retry-percent {
      type uint32 {
        range "0 .. 100";
      }
      default "80";
      description
        "cacMStreamMDirectMaxRetryPercent";
    }
    leaf mult-direct-max-stream-per-radio-val {
      type uint32 {
        range "0 .. 20";
      }
      default "0";
      description
        "This object specifies the maximum number of allowed streams per radio for a 802.11 band.";
    }
    leaf mult-direct-max-stream-per-client-val {
      type uint32 {
        range "0 .. 15";
      }
      default "0";
      description
        "This object specifies the maximum number of allowed streams per client for a 802.11 band.";
    }
  }

  container dot11-cfg-data {
    description
      "Configuration objects related to 802.11 feature";
    container dot11ac-mcs-entries {
      description
        "Configurations related to 802.11ac MCS rates";
      list dot11ac-mcs-entry {
        key "spatial-stream index";
        description
          "Configurations related to 802.11ac MCS rates";
        uses wireless-dot11-cfg:ac-mcs-80211;
        uses wireless-dot11-cfg:mcs-key-80211ac;
      }
    }
    container dot11-entries {
      description
        "Configuration related to 802.11 protocol";
      list dot11-entry {
        key "band";
        description
          "List of configuration objects related to 802.11 protocol";
        uses wireless-dot11-cfg:dot11;
      }
    }
  }
