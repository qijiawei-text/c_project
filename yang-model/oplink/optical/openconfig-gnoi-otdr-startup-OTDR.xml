<?xml version="1.0" encoding="UTF-8"?>
<optical-time-domain-reflectometer xmlns="http://openconfig.net/yang/gnoi-otdr">
        <config-common>
            <scan-types>
                <scan-type>short</scan-type>
                <range-m>5000</range-m>
                <pulse-width-ns>20</pulse-width-ns>
                <sampling-resolution-m>1</sampling-resolution-m>
        </scan-types>
            <scan-types>
                <scan-type>medium</scan-type>
                <range-m>50000</range-m>
                <pulse-width-ns>3000</pulse-width-ns>
                <sampling-resolution-m>10</sampling-resolution-m>
        </scan-types>
            <scan-types>
                <scan-type>long</scan-type>
                <range-m>125000</range-m>
                <pulse-width-ns>20000</pulse-width-ns>
                <sampling-resolution-m>40</sampling-resolution-m>
        </scan-types>
    </config-common>
        <otdrs>
            <otdr>
                <name>line-in</name>
                <parent-port>line-in</parent-port>
            </otdr>
            <otdr>
                <name>line-out</name>
                <parent-port>line-out</parent-port>
            </otdr>
            <otdr>
                <name>otdr-out</name>
                <parent-port>otdr-out</parent-port>
            </otdr>
        </otdrs>
</optical-time-domain-reflectometer>
