<?xml version="1.0" encoding="UTF-8"?>
<optical-attenuator xmlns="http://openconfig.net/yang/optical-attenuator">
    <attenuators>
        <attenuator>
            <name>PADDING-VOA</name>
            <config>
                <name>PADDING-VOA</name>
                <attenuation-mode>CONSTANT_ATTENUATION</attenuation-mode>
                <attenuation>25.0</attenuation>
                <enabled>true</enabled>
            </config>
        </attenuator>
        <attenuator>
            <name>PA-VOA</name>
            <config>
                <name>PA-VOA</name>
                <attenuation-mode>CONSTANT_ATTENUATION</attenuation-mode>
                <attenuation>25.5</attenuation>
                <enabled>true</enabled>
            </config>
        </attenuator>
        <attenuator>
            <name>OSC-TX-VOA</name>
            <config>
                <name>OSC-TX-VOA</name>
                <attenuation-mode>CONSTANT_POWER</attenuation-mode>
                <target-output-power>10</target-output-power>
                <enabled>true</enabled>
            </config>
        </attenuator>
        <attenuator>
            <name>OSC-VOA</name>
            <config>
                <name>OSC-VOA</name>
                <attenuation-mode>CONSTANT_ATTENUATION</attenuation-mode>
                <attenuation>30.0</attenuation>
                <enabled>true</enabled>
            </config>
        </attenuator>
        <attenuator>
            <name>OCM-VOA</name>
            <config>
                <name>OCM-VOA</name>
                <attenuation-mode>CONSTANT_ATTENUATION</attenuation-mode>
                <attenuation>10.0</attenuation>
                <enabled>true</enabled>
            </config>
        </attenuator>
    </attenuators>
</optical-attenuator>
