<auto-control xmlns="http://com/oplink/control-loop">
    <controls>
        <control>
            <name>W</name>
            <auto-config>
                <number-of-carriers>33</number-of-carriers>
                <launching-power-per-channel>5</launching-power-per-channel>
                <target-power-per-channel>-1.2</target-power-per-channel>
                <ocm-based-carriers>false</ocm-based-carriers>
                <fiber-type>hcf</fiber-type>
                <saturation-drop>6</saturation-drop>
                <pre-amp-input-target-power-per-channel>-8</pre-amp-input-target-power-per-channel>
            </auto-config>
            <control-threshold>
            		<span-loss-threshold>1.5</span-loss-threshold>
            </control-threshold>
        </control>
    </controls>
</auto-control>
