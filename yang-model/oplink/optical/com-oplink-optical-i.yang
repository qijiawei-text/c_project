        "Oplink Communications, LLC";

    contact
        "Oplink Communications, LLC";

    description
        "YANG definitions for internal optical control and modules";

    revision 2019-04-01 {
      description
        "Oplink Version 2.0";
    }

    uses com-oplink-optical:optical-top {
      refine optical-control/optical-modules/optical-amplifier/amplifiers/amplifier/circuit-pack-name {
