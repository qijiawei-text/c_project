      }
      refine optical-control/optical-modules/optical-time-domain-reflectometer/otdrs/otdr/component-type {
         config true;
      }
      refine optical-control/optical-modules/optical-time-domain-reflectometer/otdrs/otdr/component-index {
         config true;
